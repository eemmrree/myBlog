@using TravelTrip.Models.sınıflar
@model List<blog>
<div class="padding1">
    <h4>En Beğendiğim Yazılar</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src=@x.BlogImage alt=""  style="width:120px; height:75px"/>
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,70)...</p>
            </div>
            <div class="clear"></div>
            
        }
    </div>
    @Html.Action("Partial5","Default")
</div>